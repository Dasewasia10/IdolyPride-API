{
	"version": 2,
	"builds": [
		{
			"src": "gunicorn main:app --bind 0.0.0.0:$PORT",
			"use": "@vercel/python"
		},
		{ "src": "npm run dev", "use": "@vercel/static-build" },
		{
			"src": "requirements.txt",
			"use": "@vercel/python"
		}
	],
	"routes": [{ "src": "/api/(.*)", "dest": "api/main.py" }]
}
