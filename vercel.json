{
	"version": 2,
	"builds": [
		{
			"src": "api/main.py",
			"use": "@vercel/python"
		},
		{
			"src": "src/main.jsx",
			"use": "@vercel/static-build",
			"config": {
				"base": "/",
				"outputDirectory": "dist"
			}
		}
	],
	"routes": [
		{ "src": "/api/(.*)", "dest": "api/main.py" },
		{ "src": "/(.*)", "dest": "/dist/index.html" }
	]
}
